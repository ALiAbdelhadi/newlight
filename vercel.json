{
    "buildCommand": "pnpm run build",
    "devCommand": "pnpm run dev",
    "installCommand": "pnpm install",
    "framework": "nextjs",
    "regions": [
        "iad1"
    ],
    "env": {
        "DATABASE_URL": "@DATABASE_URL"
    },
    "build": {
        "env": {
            "PRISMA_GENERATE_SKIP_AUTOINSTALL": "false"
        }
    },
    "functions": {
        "app/**/*.tsx": {
            "maxDuration": 30
        },
        "app/api/**/*.ts": {
            "maxDuration": 30
        }
    }
}